<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="CRS.ReportExtractor.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
			<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <applicationSettings>
        <CRS.ReportExtractor.Properties.Settings>
            <setting name="ServiceURL" serializeAs="String">
                <value>http://192.168.111.11/ReportServer/ReportExecution2005.asmx</value>
            </setting>
        </CRS.ReportExtractor.Properties.Settings>
    </applicationSettings>
  <appSettings>
    <add key="ReportExtractorOutputFormat" value="\\192.168.111.11\ReportExtractorOutputFormat.txt"/>
    <add key="excludedHospitals" value="\\192.168.111.211\excludedHospitals.txt"/>
    <add key="NetworkCredentialUsername" value="ourUsername"/>
    <add key="NetworkCredentialPassword" value="ourPassword"/>
  </appSettings>
	<log4net>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Logs\Logs_Error" />
			<AppendToFile value="true" />
			<RollingStyle value="Date" />
			<DatePattern value="_yyyy.MM.dd.lo\g" />
			<StaticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %level %logger - %message%newline%exception" />
			</layout>
		</appender>
		<root>
			<level value="All" />
			<appender-ref ref="RollingFileAppender" />
		</root>
	</log4net>
</configuration>